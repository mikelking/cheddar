{
  "name": "mikelking/dummy",
  "type": "project",
  "license": "BSD3",
  "description": "A WordPress site for testing Composer based deployments",
  "homepage": "http://www.olivent.net",
  "authors": [
    {
      "name": "Olivent, llc.",
      "email": "info@olivent.com",
      "homepage": "https://github.com/mikelking/dummy"
    },
    {
      "name": "Mikel King",
      "email": "mikel.king@olivent.com",
      "homepage": "https://github.com/mikelking"
    }
  ],
  "keywords": [
    "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "config": {
    "preferred-install": {
      "*" : "dist"
    },
    "generate-salts": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mikelking/bacon"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mikelking/wordpress-config-installer"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mikelking/wpcfg"
    },
    {
      "type": "vcs",
      "url": "https://github.com/johnpbloch/wordpress-core"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mikelking/pyramid"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mikelking/virtual-ads-txt"
    },
    {
      "type": "vcs",
      "url": "https://gitlab.com/mikelking/virtual-robots-txt"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mikelking/quotes"
    },
    {
      "type": "vcs",
      "url": "https://github.com/wp-graphql/wp-graphql"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/WordPress-Coding-Standards/WordPress-Coding-Standards.git"
    }
  ],
  "extra" : {
    "wordpress-install-dir": "wproot/wordpress",
    "installer-paths" : {
      "wproot/wordpress/wp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
      "wproot/wordpress/wp-content/mu-plugins/" : ["type:wordpress-muplugin", "mikelking/bacon"],
      "wproot/wordpress/wp-content/themes/{$name}/" : ["type:wordpress-theme"]
    }
  },
  "require": {
    "php": ">=7.0",
    "composer/installers" : "v1.6.0",
    "mikelking/wpcfg" : "0.1.8",
    "johnpbloch/wordpress-core" : "5.2.3",
    "mikelking/bacon" : "1.0.17",
    "mikelking/quotes" : "0.1.4",
    "mikelking/virtual-ads-txt" : "1.0.2",
    "mikelking/virtual-robots-txt" : "1.1.0",
    "wpackagist-theme/atomic-blocks" : "1.2.5",
    "wpackagist-theme/twentyten" : "2.9.0",
    "wpackagist-theme/twentyeleven" : "3.3.0",
    "wpackagist-theme/twentytwelve" : "3.0.0",
    "wpackagist-theme/twentythirteen" : "2.9.0",
    "wpackagist-theme/twentyfourteen" : "2.7.0",
    "wpackagist-theme/twentyfifteen" : "2.5.0",
    "wpackagist-theme/twentyseventeen" : "2.2.0",
    "wpackagist-theme/twentynineteen" : "1.4.0",
    "wpackagist-theme/third-style" : "1.1.1",
    "wpackagist-plugin/addthis-all" : "3.2.3",
    "wpackagist-plugin/ajax-heartbeat-tool" : "~1.0",
    "wpackagist-plugin/akismet" : "4.1.2",
    "wpackagist-plugin/atomic-blocks" : "2.1.0",
    "wpackagist-plugin/bj-lazy-load" : "1.0.9",
    "wpackagist-plugin/classic-editor" : "1.5.0",
    "wpackagist-plugin/cloudflare" : "3.3.3",
    "wpackagist-plugin/co-authors-plus" : "3.4.0",
    "wpackagist-plugin/crayon-syntax-highlighter" : "2.8.4",
    "wpackagist-plugin/duo-wordpress" : "2.5.4",
    "wpackagist-plugin/elasticpress" : "3.1.4",
    "wpackagist-plugin/fftf-alerts" : "1.3.1",
    "wpackagist-plugin/genesis-simple-edits": "2.2.1",
    "wpackagist-plugin/genesis-simple-faq": "0.9.1",
    "wpackagist-plugin/genesis-simple-hooks": "2.2.1",
    "wpackagist-plugin/genesis-simple-sidebars": "2.2.0",
    "wpackagist-plugin/google-analytics-dashboard-for-wp" : "5.3.8",
    "wpackagist-plugin/google-analytics-for-wordpress" : "7.7.1",
    "wpackagist-plugin/gutenberg" : "5.9.2",
    "wpackagist-plugin/health-check" : "1.3.2",
    "wpackagist-plugin/jackmail-newsletters" : "1.1.2",
    "wpackagist-plugin/jetpack" : "7.4.1",
    "wpackagist-plugin/lazy-load" : "0.6.1",
    "wpackagist-plugin/media-deduper" : "1.5.2",
    "wpackagist-plugin/members" : "2.1.0",
    "wpackagist-plugin/newsletter" : "6.1.2",
    "wpackagist-plugin/ninja-forms" : "3.4.16",
    "wpackagist-plugin/page-links-to": "3.1.1",
    "wpackagist-plugin/redirection": "4.3.1",
    "wpackagist-plugin/required-fields": "1.9.5",
    "wpackagist-plugin/scheduled-tweets" : "0.0.5",
    "wpackagist-plugin/simple-iframe-buster" : "~1.0",
    "wpackagist-plugin/stream" : "3.3.0",
    "wpackagist-plugin/tablepress" : "1.9.2",
    "wpackagist-plugin/tinymce-advanced" : "5.2.1",
    "wpackagist-plugin/triberr-wordpress-plugin" : "4.0.9",
    "wpackagist-plugin/wordpress-seo": "11.4.0",
    "wpackagist-plugin/wp-bitly" : "2.5.2",
    "wpackagist-plugin/wp-optimize" : "2.3.4",
    "wp-graphql/wp-graphql" : "0.3.3",
    "mikelking/pyramid": "0.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.6",
    "wpackagist-plugin/show-current-template": "0.3.0",
    "phpmd/phpmd" : "2.6.0",
    "squizlabs/php_codesniffer": "3.4.0",
    "phploc/phploc": "4.0.1",
    "sebastian/phpcpd": "4.1.0",
    "wp-cli/wp-cli-bundle": "2.1.0",
    "phan/phan": "1.2.6"
  }
}
