{
  "name": "mikelking/dummy",
  "type": "project",
  "license": "BSD3",
  "description": "A WordPress site for testing Composer based deployments",
  "homepage": "http://www.olivent.net",
  "authors": [
    {
      "name": "Olivent, llc.",
      "email": "info@olivent.com",
      "homepage": "https://github.com/mikelking/dummy"
    },
    {
      "name": "Mikel King",
      "email": "mikel.king@olivent.com",
      "homepage": "https://github.com/mikelking"
    }
  ],
  "keywords": [
    "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "config": {
    "preferred-install": "source",
    "generate-salts": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mikelking/bacon"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mikelking/wordpress-config-installer"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mikelking/wpcfg"
    },
    {
        "type": "vcs",
        "url": "https://github.com/ReadersDigest/genesis"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ReadersDigest/altitude-pro"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ReadersDigest/magazine-pro"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ReadersDigest/news-pro"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ReadersDigest/wp-ultimate-recipe-premium"
    },
    {
        "type": "vcs",
        "url": "https://github.com/ReadersDigest/advanced-custom-fields-pro"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ReadersDigest/wp-migrate-db-pro"
    },
    {
        "type": "vcs",
        "url": "https://github.com/ReadersDigest/wp-migrate-db-pro-cli"
    },
    {
        "type": "vcs",
        "url": "https://github.com/ReadersDigest/wp-migrate-db-pro-media-files"
    },
    {
        "type": "vcs",
        "url": "https://github.com/ReadersDigest/wp-migrate-db-pro-multisite-tools"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra" : {
    "wordpress-install-dir": "wproot/wordpress",
    "installer-paths" : {
      "wproot/wordpress/wp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
      "wproot/wordpress/wp-content/mu-plugins/" : ["type:wordpress-muplugin", "mikelking/bacon"],
      "wproot/wordpress/wp-content/themes/{$name}/" : ["type:wordpress-theme"]
    }
  },
  "require": {
    "php": ">=5.5",
    "composer/installers" : "v1.2.0",
    "mikelking/wpcfg" : "~0.1.0",
    "johnpbloch/wordpress" : "~4.9.1",
    "mikelking/bacon" : "^1.0.0",
    "ReadersDigest/genesis" : "~2.5.3",
    "ReadersDigest/altitude-pro" : "~1.1.2",
    "ReadersDigest/magazine-pro" : "~3.2.3",
    "ReadersDigest/news-pro" : "~3.2.1",
    "wpackagist-theme/twentyseventeen" : "*",
    "ReadersDigest/advanced-custom-fields-pro" : "~5.5.2",
    "ReadersDigest/wp-ultimate-recipe-premium" : "~3.6.1",
    "ReadersDigest/wp-migrate-db-pro" : "~1.8.1",
    "ReadersDigest/wp-migrate-db-pro-cli" : "~1.3.2",
    "ReadersDigest/wp-migrate-db-pro-multisite-tools" : "~1.2",
    "ReadersDigest/wp-migrate-db-pro-media-files" : "~1.4.9",
    "wpackagist-plugin/wordpress-seo" : "~7.0",
    "wpackagist-plugin/akismet" : "dev-trunk",
    "wpackagist-plugin/co-authors-plus" : "~3.2.2",
    "wpackagist-plugin/lazy-load" : "~0.6.1",
    "wpackagist-plugin/members" : "~1.1.3",
    "wpackagist-plugin/wp-bitly" : "~1.0",
    "wpackagist-plugin/tinymce-advanced" : "~4.6.3",
    "wpackagist-plugin/ajax-heartbeat-tool" : "~1.0",
    "wpackagist-plugin/simple-iframe-buster" : "~1.0",
    "wpackagist-plugin/duo-wordpress" : "~2.5.4",
    "wpackagist-plugin/newsletter" : "~5.1.0",
    "wpackagist-plugin/crayon-syntax-highlighter" : "~2.8.4"
  }
}
